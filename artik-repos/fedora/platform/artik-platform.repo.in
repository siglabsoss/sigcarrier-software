[artik-platform]
name=_TARGET_NAME_ platform - $basearch
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/_RELEASE_VERSION_/Everything/$basearch/os/
enabled=1
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform

[artik-platform-debuginfo]
name=_TARGET_NAME_ platform - $basearch - Debug
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/_RELEASE_VERSION_/Everything/$basearch/debug/
enabled=0
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform

[artik-platform-source]
name=_TARGET_NAME_ platform - $basearch - Source
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/_RELEASE_VERSION_/Everything/source/SRPMS/
enabled=0
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform
