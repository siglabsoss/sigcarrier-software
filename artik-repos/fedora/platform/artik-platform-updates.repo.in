[artik-platform-updates]
name=_TARGET_NAME_ platform - $basearch - Updates
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/updates/$basearch/
enabled=1
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform

[artik-platform-updates-debuginfo]
name=_TARGET_NAME platform - $basearch - Updates - Debug
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/updates/$basearch/debug/
enabled=0
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform

[artik-platform-updates-source]
name=_TARGET_NAME_ platform - $basearch - Updates - Source
failovermethod=priority
baseurl=http://repo.artik.cloud/artik/bin/pub/artik-platform/fedora/_TARGET_NAME_/updates/source/SRPMS/
enabled=0
priority=50
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-artik-platform
